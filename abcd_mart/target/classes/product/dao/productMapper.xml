<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="productSQL">
	<insert id="applyPdt" parameterType="map">
		insert into 
					product
		values(
			#{prdtMainName},
			#{prdtMiniName},
			#{prdtStyleCode},
			#{prdtCode},
			#{prdtPrice },
			#{prdtDeliveryPrice },
			#{prdtBrand },
			#{prdtColor },
			#{prdtGender },
			#{prdtSize},
			0)
	</insert>
</mapper>